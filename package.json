{
  "name": "theme",
  "displayName": "Theme",
  "description": "A Visual Studio Code theme  for those of us who like to code late into the night",
  "version": "2.1.1",
  "publisher": "tal7aouy",
  "author": {
    "name": "MHAMMED TALHAOUY",
    "email": "talhaouycoding@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tal7aouy/theme"
  },
  "galleryBanner": {
    "color": "#282c34",
    "theme": "dark"
  },
  "scripts": {
    "build": "ts-node src/index.ts",
    "package": "vsce package",
    "start": "nodemon",
    "lint": "eslint src --ext ts"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/tal7aouy/theme/issues"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "icon": "logo.png",
  "keywords": [
    "Theme",
    "Dark Theme",
    "Customize",
    "Professional",
    "One Theme",
    "One Dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-color.json"
      },
      {
        "label": "Theme (Italic)",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-color-italic.json"
      }
    ]
  },
  "dependencies": {
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin-tslint": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vsce": "^2.6.7"
  }
}